#include<stdio.h>

int main() {
	int low_price = 2001;
	int hamburger = 0;
	int low_bever = 2001;
	int beverege = 0;
	for (int i = 0; i < 3; i++) {
		scanf("%d", &hamburger);
		if (low_price > hamburger)
			low_price = hamburger;
	}
	for (int i = 0; i < 2; i++) {
		scanf("%d", &beverege);
		if (low_bever > beverege)
			low_bever = beverege;
	}
	printf("%d", low_price + low_bever - 50);
	return 0;
}
